          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="JSR223 PostProcessor">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">// Si hubo 409, refresca versión y reintenta 1 vez
if (prev.getResponseCode() == &apos;409&apos;) {
    log.warn(&apos;409 detectado; refrescando versión y reintentando PUT una vez&apos;)
    // Llama sincrónicamente al GET para actualizar version
    def getRes = org.apache.jmeter.protocol.http.sampler.HTTPSamplerFactory.newInstance()
    getRes.setDomain(vars.get(&apos;host&apos;))
    getRes.setPort(Integer.parseInt(vars.get(&apos;port&apos;)))
    getRes.setProtocol(vars.get(&apos;protocol&apos;))
    getRes.setPath(&quot;/orders/${vars.get(&apos;orderId&apos;)}&quot;)
    getRes.setMethod(&quot;GET&quot;)
    getRes.setUseKeepAlive(true)
    def res = getRes.sample(context)
    def json = new groovy.json.JsonSlurper().parseText(res.getResponseDataAsString())
    vars.put(&apos;orderVersion&apos;, String.valueOf(json.version as int))

    // Señala este sample como fallido para que un Retry Controller o un bucle controlado lo reprocese
    prev.setSuccessful(false)
}
</stringProp>
          </JSR223PostProcessor>
